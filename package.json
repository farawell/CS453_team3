{
  "devDependencies": {
    "prettier": "^3.3.3",
    "pyright": "^1.1.375"
  },
  "scripts": {
    "pyright": "pyright",
    "serve": "cd app && streamlit run main.py",
    "genai": "npx --yes genaiscript run",
    "gcm": "npx --yes genaiscript run gcm",
    "prd": "npx --yes genaiscript run prd -prd",
    "build": "npx --yes genaiscript scripts compile",
    "promptpex": "npx --yes genaiscript run promptpex",
    "execute_tests:ollama": "npx --yes genaiscript run promptpex_execute_tests --vars models=ollama:phi3.5;ollama:qwen2.5-coder:latest;ollama:llama3.2:3b --vars concurrency=1 --model ollama:phi3.5",
    "evaluate_tests": "npx --yes genaiscript run promptpex_evaluate_tests"
  }
}
